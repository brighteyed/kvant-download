    "version": "0.2.0",
    "configurations": [
        {
            "name": "Kvant",
            "type": "python",
            "request": "launch",
            "module": "scrapy",
            "cwd": "${workspaceFolder}/spiders",
            "args": [
                "crawl",
                "kvant",
                "-o",
                "..\\kvant.json"
            ],
            "console": "integratedTerminal",
            "preLaunchTask": "clean:kvant"
        },
        {
            "name": "Kvantik",
            "type": "python",
            "request": "launch",
            "module": "scrapy",
            "cwd": "${workspaceFolder}/spiders",
            "args": [
                "crawl",
                "kvantik",
                "-o",
                "..\\kvantik.json"
            ],
            "console": "integratedTerminal",
            "preLaunchTask": "clean:kvantik"
        }
    ]
}